<div class="container">
<h2><%= @city&.name%></h2>
  <div class="row" style="margin: 1px;">
      <% @categories&.each do |cat|%>
      <div class="col-md-4">
        <li style="list-style-type: square; color: red; font-size: 1em;"><%= cat.name%></li>
        <%cat.sub_categories.each do |subc|%>
        	<p><%= link_to subc&.name, dashboard_decide_path(cat_id: subc.id, city_id: @city.id, category_id: cat.id), :style => "color: white; font-size: 15px;"%></p>
        <%end%>
        </div>
      <%end%>
  </div>
</div>